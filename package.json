{
  "name": "sharp-transform-loader",
  "version": "3.0.0",
  "description": "Sharp transform loader module for webpack",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "start": "npm run build -- -w",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js' --copy-files",
    "clean": "rm -rf dist",
    "example": "webpack --mode=production --config example/pure/webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">= 16.13.0"
  },
  "author": "Alex Olexyuk <olexuk.alex@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/11gorizont11/sharp-transform-loader"
  },
  "peerDependencies": {
    "file-loader": "^3.0.0"
  },
  "dependencies": {
    "loader-utils": "^1.2.2",
    "schema-utils": "^1.0.0",
    "sharp": "^0.29.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@webpack-contrib/defaults": "^3.0.0",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.0.0-beta.6",
    "cross-env": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "file-loader": "^3.0.1",
    "jest": "^27.4.3",
    "jsdom": "^9.12.0",
    "lint-staged": "^11.0.0",
    "memory-fs": "^0.4.1",
    "prettier": "^1.8.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2"
  },
  "keywords": [
    "webpack"
  ],
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.9.0"
          },
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ]
  }
}